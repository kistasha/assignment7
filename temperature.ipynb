   ],
   "source": [
    "celsius=float(input(\"Enter temperature in Celsius: \"))\n",
    "fahrenheit=(celsius*9/5)+32\n",
    "\n",
    "print(str(celsius)+\" degrees celsius is equal to \"+str(fahrenheit)+\" degrees fahrenheit\")"
   ]
